    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/tontxns.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "TonTxns"
    }
  },

  "ComponentLaunchSettings": {
    "httpsUrl": "https://*:55006",
    "httpUrl": "http://*:56006"
  },

  "MySql": {
    "Server": "localhost",
    "User": "user",
    "Password": "userPass",
    "DbName": "Windetta.TonTxns",
    "Version": "8.0",
    "Port": 3506
  },

  "WalletCredential": {
    "PublicKey": "uGfokUoBe1eC2EmWX0JoOdbb6Gim+uxBTDzaN83cEBs=",
    "PrivateKey": "ilC4FwyMYJm6t29BXc7EJsvMjmLIQGSi0iXVIjb6apk="
  },

  "TonDepositAddress": "EQCY-EUrQiEgy8a2JI5PNfv3itWseEh1TtzrPsIRDpH8iS2H",

  "TonApi": {
    "Endpoint": "http://localhost:8081/jsonRPC",
    "ApiKey": "xxx"
  },

  "EncryptionOptions": {
    "PrivateKey": "Ie7rHStIH0CnvuHWVWu23PCSY4aWNVZ0",
    "Vector": "Z2B8DsKjCrUQe60U"
  },

  "RabbitMq": {
    "namespace": "ton-txns",
    "retries": 3,
    "retryInterval": 2,
    "username": "admin",
    "password": "@admin@",
    "virtualHost": "/",
    "port": 5674,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
